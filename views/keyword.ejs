<script>
  $(function(){
    count = 0
    $("#hov-list").hide();
    $("#hide-box").hide();

    $("#searcharea").on('keypress',function(e){
      if (e.which == 13){
        console.log($(this).val())
        $("#tagcontainer").prepend("<span class='tag' id='new'>" + $(this).val() + "   <i class='icon ion-backspace'></i></span>");
        $(this).val("")
        var org_width = $(this).css("width")
        var new_width = $("#new").css("width")
        if (count == 0){
          $("#searcharea").css("width", "149")
          count = count + 1
        }else{
          $("#searcharea").css("width", "50")
        }
        $("#searcharea").attr("placeholder","")
      }
    });
    $("#searcharea").on('focus',function(e){
      $("#hov-list").show()
    });
    $("#click-key").on('click',function(e){
      e.preventDefault();
      $("#show-box").hide()
      $("#hide-box").show()

    })
    $("#link-heart").hover(function(){
      $(this).removeClass('ion-ios-heart-outline')
      $(this).addClass('ion-ios-heart')
    })
    $("#link-heart").on('mouseleave',function(){
      if ($(this).hasClass('clicked-link')){
      }else{
        $(this).removeClass('ion-ios-heart')
        $(this).addClass('ion-ios-heart-outline')
      }
    })
    $("#link-heart").on('click',function(){
      $(this).addClass('ion-ios-heart clicked-link')
      $(this).css('color','red')
    })

    $("#link-bookmark").hover(function(){
      $(this).removeClass('ion-ios-bookmarks-outline')
      $(this).addClass('ion-ios-bookmarks')
    })
    $("#link-bookmark").on('mouseleave',function(){
      if ($(this).hasClass('clicked-link')){
      }else{
        $(this).removeClass('ion-ios-bookmarks')
        $(this).addClass('ion-ios-bookmarks-outline')
      }
    })
     $("#link-bookmark").on('click',function(){
      $(this).addClass('ion-ios-bookmarks clicked-link')
      $(this).css('color','orange')
    })


